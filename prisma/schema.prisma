generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model Notes {
  noteid String @id @map("_id")
  title   String   
  crDate  DateTime @default(now())
  upDate  DateTime @updatedAt
  content String   
  stared  Boolean  @default(false)
}